    "// ============================================================": "",
    "// GOOGLE WORKSPACE TOOLS": "",
    "// Complete suite of Google Workspace integrations": "",
    "// ============================================================": "",

    "google-gmail": {
      "command": "uvx",
      "args": [
        "automagik-tools@latest",
        "tool",
        "google-gmail",
        "--transport",
        "stdio"
      ],
      "disabled": false,
      "alwaysAllow": ["call_tool"],
      "env": {
        "GOOGLE_GMAIL_CLIENT_ID": "your-client-id.apps.googleusercontent.com",
        "GOOGLE_GMAIL_CLIENT_SECRET": "GOCSPX-your-client-secret",
        "GOOGLE_GMAIL_CREDENTIALS_DIR": "/path/to/.credentials/google-workspace",
        "GOOGLE_GMAIL_USER_EMAIL": "your-email@example.com",
        "GOOGLE_GMAIL_ENABLE_OAUTH21": "false",
        "GOOGLE_GMAIL_SINGLE_USER_MODE": "true",
        "GOOGLE_GMAIL_STATELESS_MODE": "false",
        "GOOGLE_GMAIL_BASE_URI": "http://localhost",
        "GOOGLE_GMAIL_PORT": "8001",
        "GOOGLE_GMAIL_LOG_LEVEL": "INFO",
        "GOOGLE_OAUTH_REDIRECT_URI": "http://localhost:8001/oauth2callback"
      }
    },

