      "env": {
        "OMNI_API_KEY": "your_api_key_here",
        "OMNI_BASE_URL": "http://localhost:8882",
        "OMNI_DEFAULT_INSTANCE": "genie",
        "OMNI_TIMEOUT": "30",
        "OMNI_MAX_RETRIES": "3",
        "OMNI_MODE": "agent_owned",
        "OMNI_MEDIA_DOWNLOAD_FOLDER": "/tmp/genie-omni-media",
        "// Security: Context Isolation (RECOMMENDED!)": "",
        "// Set one of these to limit agent to specific contact/group": "",
        "OMNI_MASTER_PHONE": "",
        "OMNI_MASTER_GROUP": ""
      }
