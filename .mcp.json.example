    "genie": {
      "command": "uvx",
      "args": [
        "automagik-tools@latest",
        "tool",
        "genie",
        "--transport",
        "stdio"
      ],
      "disabled": false,
      "alwaysAllow": ["call_tool"],
      "env": {
        "OPENAI_API_KEY": "your_openai_api_key",
        "GENIE_MODEL": "gpt-4.1",
        "GENIE_MEMORY_DB": "genie_memory.db",
        "GENIE_STORAGE_DB": "genie_storage.db",
        "GENIE_SESSION_ID": "global_genie_session",
        "GENIE_HISTORY_RUNS": "3",
        "GENIE_SHOW_TOOL_CALLS": "true",
        "// MCP Server Management": "",
        "GENIE_MCP_CLEANUP_TIMEOUT": "2.0",
        "GENIE_SSE_CLEANUP_DELAY": "0.2",
        "GENIE_AGGRESSIVE_CLEANUP": "true",
        "// Configure MCP servers for Genie to orchestrate": "",
        "// Example: GENIE_MCP_CONFIGS": "{\"filesystem\": {\"command\": \"npx\", \"args\": [\"-y\", \"@modelcontextprotocol/server-filesystem\", \"/tmp\"]}}"
      }
    },

